extern printf
extern lotto
extern time
extern srand

section .text
global main
main:
    ;write your code here
    push 0
    call time 
    add esp, 4
    
    push eax
    call srand
    add esp, 4
    
    mov ebx, 0

find_lotto:

    call lotto
    mov ecx, 0
    
check_loop:

    cmp ecx, ebx
    jge found_lotto
    cmp [you+4*ecx], eax
    je find_lotto
    inc ecx
    jmp check_loop

found_lotto:

    mov [you+4*ebx], eax
    inc ebx
    cmp ebx, 6
    jl find_lotto

print_lotto:

    push dword [you]
    push dword [you+4]
    push dword [you+8]
    push dword [you+12]
    push dword [you+16]
    push dword [you+20]  
    push msg
    call printf
    add esp, 28
        
    xor eax, eax
    ret


section .data
    msg db 'Lotto : %d, %d, %d, %d, %d, %d'
section .bss
    you resd 6
